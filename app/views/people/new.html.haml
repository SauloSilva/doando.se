%h1= t('views.people.subscribe')

#subscribe
  = render 'shared/social'

  = simple_form_for setup_people(@person), :html => { multipart: true } do |f|
    = render partial: 'form', locals: { f: f }

    .actions
      = f.submit t('buttons.new'), class: 'button red'

- content_for :javascript do
  :javascript
    window.social = new SocialButtons();
    new IdentityForm()